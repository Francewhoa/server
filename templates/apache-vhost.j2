
<VirtualHost *:80>
    DocumentRoot    "/var/hubdrop/app/web"
    DirectoryIndex  index.php index.html
    SetEnv          SYMFONY__APP__GITHUB__USERNAME hubdrop-user
    SetEnv          SYMFONY__APP__GITHUB__ORGANIZATION drupalprojects
    SetEnv          SYMFONY__APP__DRUPAL__USER hubdrop

    <Directory "/var/hubdrop/app/web">
        AllowOverride All
        Allow from All
    </Directory>
</VirtualHost>
